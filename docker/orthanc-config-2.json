{
  "Name": "Orthanc Test PACS 2",
  "StorageDirectory": "/var/lib/orthanc/db",
  "IndexDirectory": "/var/lib/orthanc/db",
  "StorageCompression": false,
  "MaximumStorageSize": 0,
  "MaximumPatientCount": 0,
  
  "HttpServerEnabled": true,
  "HttpPort": 8042,
  "HttpDescribeErrors": true,
  "HttpCompressionEnabled": true,
  
  "DicomServerEnabled": true,
  "DicomAet": "TESTPACS2",
  "DicomCheckCalledAet": false,
  "DicomPort": 4242,
  "DefaultEncoding": "Latin1",
  "AcceptedTransferSyntaxes": ["1.2.840.10008.1.*"],
  
  "UnknownSopClassAccepted": false,
  "DicomAlwaysAllowEcho": true,
  "DicomAlwaysAllowStore": true,
  "DicomAlwaysAllowFind": true,
  "DicomCheckModalityHost": false,
  "DicomScpTimeout": 30,
  
  "DicomModalities": {
    "DICOMFAB": ["DICOMFAB", "host.docker.internal", 11112],
    "SELF": ["TESTPACS2", "localhost", 4242],
    "TESTPACS": ["TESTPACS", "orthanc-test-pacs", 4242]
  },
  
  "RegisteredUsers": {
    "test2": "test456"
  },
  
  "AuthenticationEnabled": true,
  
  "RemoteAccessAllowed": true,
  
  "Plugins": [
    "/usr/share/orthanc/plugins",
    "/usr/local/share/orthanc/plugins"
  ],
  
  "WebDavEnabled": true,
  "WebDavDeleteAllowed": true,
  "WebDavUploadAllowed": true,
  
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "EnableWado": true,
    "WadoRoot": "/wado",
    "Ssl": false,
    "StudiesMetadata": "Full",
    "SeriesMetadata": "Full"
  }
}